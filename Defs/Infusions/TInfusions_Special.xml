    <Infusion.InfusionDef ParentName="InfusionRepurposed1Ranged">
        <defName>TI_RRapidFire1</defName>
        <label>Rapid Fire 1</label>
        <labelShort>rfire1</labelShort>
        <stats>
            <li>
                <key>RangedWeapon_Cooldown</key>
                <value>
                    <multiplier>-0.75</multiplier>
                </value>
            </li>
